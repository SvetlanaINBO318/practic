#include <iostream>
#include <cmath>
#include <iomanip>
#include <cstring>
using namespace std;

struct Book{
    int year;               // год выпуска
    string author;          //автор
    int pages;              //кол-во страниц
    string name;            //название
    void display(){ cout<<"Year: "<<year<<endl;         //вывод
                  cout<<"Author: "<<author<<endl;
                  cout<<"Pages: "<<pages<<endl;
                  cout<<"Name: "<<name<<endl;}
};

int main()
{
   Book one={1859,(string)"Goncharov", 510,(string) "Oblomov"};
   one.display();
   return 0;
}

