    string author;          //автор
    int pages;              //кол-во страниц
    string name;            //название

struct Biblioteca{
    Book a[20];
};

int main()
{
    Biblioteca one={{1859,(string)"Goncharov", 510,(string) "Oblomov"},{1859,(string)"Goncharov", 510,(string) "Oblomov"}};
    return 0;
}
